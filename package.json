{
  "name": "morpho-data-structures",
  "version": "1.0.0",
  "description": "Data structures for Morpho's matching engine",
  "main": "hardhat.config.js",
  "repository": "https://github.com/morpho-protocol/morpho-data-structures.git",
  "author": "MerlinEgalite <egalite.merlin@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "npx hardhat test",
    "coverage": "npx hardhat coverage",
    "lint": "yarn run lint:sol && yarn run lint:js",
    "lint:js": "eslint .",
    "lint:sol": "solhint contracts/**/*.sol",
    "fix": "yarn fix:sol && yarn fix:js",
    "fix:js": "eslint --fix **/*.js",
    "fix:sol": "prettier --write contracts/**/*.sol",
    "size": "yarn run hardhat size-contracts",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.1",
    "hardhat": "^2.5.0",
    "hardhat-gas-reporter": "^1.0.4",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "^0.7.16"
  }
}
